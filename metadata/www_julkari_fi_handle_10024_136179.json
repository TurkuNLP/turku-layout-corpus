metadata/julkari.jsonl:{"FLocat": [null, null], "abstract": ["Hoitotyön luonne on muuttunut yhä enemmän aivotyöksi, jossa tiedolla työskentely ja työn kognitiiviset eli tiedonkäsittelyn vaatimukset ovat kasvaneet voimakkaasti. Hoitotyössä on paljon myös keskeytyksiä ja useita samanaikaisia tehtäviä. Ne ovat kognitiivisia kuormitustekijöitä, jotka heikentävät työn sujuvuutta ja työhyvinvointia.\r\n \r\nTämän kehittämishankkeen lähtökohtana oli Satakunnan sairaanhoitopiirissä esille noussut tarve parantaa henkilöstön työhyvinvointia ja vähentää kuormittumista, jonka taustalla vaikutti olevan erityisesti kognitiiviseen ergonomiaan liittyviä ongelmia. Kognitiivista ergonomiaa parannetaan suunnittelemalla työprosessit siten, että ne ovat sopusoinnussa inhimillisen tiedonkäsittelyn kykyjen ja rajoitusten kanssa. Lisäksi vaikutetaan kognitiivisesti kuormittaviin työolosuhteisiin ja näin vähennetään tarpeetonta aivokuormaa."], "accessCondition": ["cc by 4.0"], "agent": [null], "amdSec": [null, null, null], "binData": [""], "dateAccessioned": ["2018-04-03T07:58:51Z"], "dateAvailable": ["2018-04-03T07:58:51Z"], "dateIssued": ["2018-04"], "div": [null, null], "dmdSec": [null], "extension": [null, null], "file": [null, null], "fileGrp": [null, null], "fileSec": [null], "fixity": [null, null], "format": [null, null], "formatDesignation": [null, null], "formatName": ["application/pdf", "text/plain"], "fptr": [null], "genre": [null], "identifier": ["978-952-261-810-8", "http://www.julkari.fi/handle/10024/136179", "URN:ISBN:978-952-261-810-8"], "language": [null], "languageTerm": ["fin"], "mdWrap": [null, null, null, null], "messageDigest": ["fa6310d175c3477ac1565c9a347bf5c2", "709a31e5ba480ccb8b057dca84353691"], "messageDigestAlgorithm": ["MD5", "MD5"], "metsHdr": [null], "mods": [null], "name": ["Julkari", null, null, null, null, null, null, null], "namePart": ["Kalakoski, Virpi", "Käpykangas, Sari", "Valtonen, Teppo", "Selinheimo, Sanna", "Koivisto, Tiina", "Paajanen, Teemu", "L